pipeline {
  agent any
  stages {
    stage('Build Assets') {
      agent any
      steps {
        echo 'fetch and build'
      }
      node {
        git url: 'https://github.com/jglick/simple-maven-project-with-tests.git'
        def mvnHome = tool 'M3'
        sh "${mvnHome}/bin/mvn -B verify"
      }
    }
  }
}
